{
  "name": "nxplorerjs-microservice",
  "version": "1.5.2",
  "description": "Express Microservice Starter Project",
  "main": "index.js",
  "scripts": {
    "start": "node build/main",
    "start:docker": "node main",
    "compile": "cross-env NODE_ENV=production node build.js && backpack build",
    "dev": "cross-env NODE_ENV=development node build.js && backpack dev",
    "dash": "nodejs-dashboard -- node -r nodejs-dashboard build/main",
    "test": "cross-env NODE_ENV=test node build.js && npm run lint && jest --forceExit --config unit.config.json",
    "lint": "npm run tslint -- -o reports/lint_issues.json -t json --force \"server/**/*.ts\" ",
    "lint:console": "npm run tslint -- --force \"server/**/*.ts\" ",
    "tslint": "tslint",
    "wait-port": "wait-port",
    "sonar-scanner": "sonar-scanner",
    "itest:build": "cross-env NODE_ENV=test node build.js && backpack build",
    "itest:run": "concurrent --kill-others --success first \"cd build && node main\" \"wait-port localhost:3000 && jest --forceExit --config itest.config.json\""
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4,
    "sonar56x": true
  },
  "dependencies": {
    "apollo-errors": "^1.5.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "brakes": "^2.5.3",
    "casual": "^1.5.19",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dataloader": "^1.3.0",
    "dotenv": "^5.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "express-partial-response": "^0.3.4",
    "graphql": "^0.12.3",
    "graphql-playground-middleware-express": "^1.3.8",
    "graphql-server-express": "^1.3.1",
    "graphql-subscriptions": "^0.5.6",
    "graphql-tools": "^2.14.0",
    "helmet": "^3.6.1",
    "inversify": "^4.10.0",
    "inversify-express-utils": "^5.2.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.5",
    "node-fetch": "^1.7.3",
    "node-rx-http": "^1.0.1",
    "pino": "^4.8.1",
    "prom-client": "^10.1.1",
    "randexp": "^0.4.6",
    "reflect-metadata": "^0.1.10",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "response-time": "^2.3.2",
    "rxjs": "^5.5.6",
    "source-map-support": "^0.5.0",
    "subscriptions-transport-ws": "^0.9.4",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "swagger-ui-express": "^2.0.14",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/jest": "^22.0.1",
    "@types/request": "^2.0.0",
    "@types/supertest": "^2.0.3",
    "@types/zen-observable": "^0.5.3",
    "apollo-link": "^1.0.7",
    "awesome-typescript-loader": "^3.4.1",
    "backpack-core": "^0.5.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "graphql-tester": "0.0.5",
    "jest": "^22.0.6",
    "jest-sonar-reporter": "^1.3.0",
    "nodejs-dashboard": "^0.4.3",
    "shelljs": "^0.8.0",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.0.1",
    "tslint": "^5.3.2",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.9.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^2.6.2",
    "wait-port": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ERS-HCL/nxplorerjs-microservice-starter.git"
  },
  "keywords": [
    "cloud",
    "native",
    "microservice",
    "express",
    "reactive",
    "12-factor",
    "graphql",
    "jest"
  ],
  "author": "Tarun Kumar Sukhu <tksukhu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ERS-HCL/nxplorerjs-microservice-starter/issues"
  },
  "homepage": "https://github.com/ERS-HCL/nxplorerjs-microservice-starter#readme"
}
